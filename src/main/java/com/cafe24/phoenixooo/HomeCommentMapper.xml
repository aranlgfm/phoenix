<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.cafe24.phoenixooo.HomeCommentMapper">

<insert id="insertTest" parameterType="com.cafe24.phoenixooo.TestModel">
	INSERT INTO
	home_comment(
		comment_writer
		,comment_pw
		,comment_content
		,comment_date
	)VALUES(
		#{commentWriter}
		,#{commentPw}
		,#{commentContent}
		,NOW()
	)
</insert>

</mapper>